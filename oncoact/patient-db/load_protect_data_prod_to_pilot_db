#!/usr/bin/env bash

source locate_files || exit 1
source metadata_functions || exit 1
source message_functions || exit 1
source database_functions || exit 1

run=$1 && shift
sample=$1 && shift

if [[ -z "${run}" ]]; then
    error "No run passed. Exiting"
fi

if [[ -z "${sample}" ]]; then
    error "No sample passed. Exiting"
fi
protect_evidence_tsv=$(find -L /home/lschoenmaker/tmp/verifications/verification_reporting_pipeline_v1.0.5_ckb_clinical_trials/reporting_pipeline_data_v2/verification/${run}/protect -type f -name "protect.tsv")


database="protect_pilot"
credentials=$(prod_writer_sql_credentials)
patient_db_jar="/data/experiments/pilot_tools/patient-db/oncoact-db-jar-with-dependencies.jar"

do_load_protect_data ${sample} ${protect_evidence_tsv} ${database} "${credentials}" ${patient_db_jar}