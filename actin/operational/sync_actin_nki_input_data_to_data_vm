#!/usr/bin/env bash

source locate_files || exit 1
source message_functions || exit 1
source actin_config || exit 1

if [[ $(hostname) != "data-vm-prod-2" ]]; then
    error "NKI sync is only supported on data-vm-prod-2"
fi

sync_actin_input_data_to_vm $(production_actin_nki_project) $(locate_actin_clinical_feed_input_bucket ${gcp_project})

