#!/usr/bin/env bash

source locate_files || exit 1
source message_functions || exit 1
source actin_config || exit 1

if [[ $(hostname) != "$(pilot_actin_vm)" ]]; then
    error "NKI data-vm sync is only supported on pilot ACTIN VM!"
fi

do_sync_actin_input_data_to_vm $(production_actin_nki_project) $(locate_actin_clinical_feed_input_bucket $(production_actin_nki_project))

