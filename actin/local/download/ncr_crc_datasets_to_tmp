#!/usr/bin/env bash

source message_functions || exit 1

info "Downloading NCR CRC datasets to local"

gsutil cp "gs://hmf-crunch-actin-ncr-dataset/crc/delivery1/K23244.csv" "${HOME}/hmf/tmp/ncr_crc_dataset_1.csv"
gsutil cp "gs://hmf-crunch-actin-ncr-dataset/crc/delivery2/K2400223.csv" "${HOME}/hmf/tmp/ncr_crc_dataset_2.csv"